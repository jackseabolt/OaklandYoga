<div class="container-fluid content">
	<div id="banner" class="row">
		<div class="col-sm-12 text-center">
			<h1 id="logo">Oakland Yoga</span></h1>
			<%= link_to "Buy Classes", root_path, class:'btn btn-lg btn-primary' %>
		</div>
	</div>
	<div class="row center_section">
		<div class="col-sm-8 col-sm-push-2" style="text-align:center">

		</div>
	</div>
	<div class="row contact_section">
		<div class="col-sm-10 col-sm-push-1">
			<div class="col-md-12">
				<div class="col-md-12">
					<h3>Contact Us</h3>
					<h5>If you have questions of would like to get in contact with us, please fill out this form</h5>
					<br />
					<div class="form-group">
						<input class="form-control" placeholder="Name">
					</div>
					<div class="form-group">
						<input class="form-control" placeholder="Email">
					</div>
					<div class="form-group">
						<textarea class="form-control" rows="7" placeholder="Your message"></textarea>
					</div>
					<div class="form-group">
						<button class="btn btn-md btn-primary">Send</button>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>